{
	"name": "fluig-vscode-extension",
	"displayName": "fluig-vscode-extension",
	"description": "Extensão para agilizar o desenvolvimento para o Fluig no VS Code",
	"version": "0.0.1",
	"publisher": "BrunoGasparetto",
	"repository": {"url": "https://github.com/brunogasparetto/fluig-vscode-extension"},
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:fluig-vscode-extension.newDataset",
		"onCommand:fluig-vscode-extension.newForm",
		"onCommand:fluig-vscode-extension.newFormEvent"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "fluig-vscode-extension.newDataset",
				"title": "Novo Dataset",
				"category": "Fluig"
			},
			{
				"command": "fluig-vscode-extension.newForm",
				"title": "Novo Formulário",
				"category": "Fluig"
			},
			{
				"command": "fluig-vscode-extension.newFormEvent",
				"title": "Novo Evento de Formulário",
				"category": "Fluig"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "fluig-vscode-extension.newFormEvent",
					"when": "false"
				}
			],
			"explorer/context": [
				{
					"command": "fluig-vscode-extension.newDataset",
					"group": "navigation@-1"
				},
				{
					"command": "fluig-vscode-extension.newForm",
					"group": "navigation@-1"
				},
				{
					"command": "fluig-vscode-extension.newFormEvent",
					"when": "explorerResourceIsFolder || isFileSystemResource",
					"group": "navigation@-1"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.54.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"ts-loader": "^8.0.14",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	}
}
